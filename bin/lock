#!/bin/bash

i3lock --color=202020 \
       --ring-color=ff0000ff \
       --insidever-color=00000000 \
       --inside-color=00000000 \
       --time-color=ff0000ff \
       --date-color=ff0000ff \
       --ringwrong-color=00000000 \
       --wrong-color=000000ff \
       --verif-text="Yee" \
       --wrong-text="Nah" \
       --time-str="%I:%M:%S %P" \
       --radius 120 \
       --date-str="%F" \
       --blur=6 \
       -f \
       -k
